<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_margin="20dp"
    >

    <Button
        android:id="@+id/button_room"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="room" />

    <Button
        android:id="@+id/button_paging"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="local paging3" />
    <Button
        android:id="@+id/button_network_paging"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="network paging3" />
    <Button
        android:id="@+id/button_mediator_paging"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="mediator paging3" />
    <Button
        android:id="@+id/button_article_paging"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="article paging3" />
</LinearLayout>